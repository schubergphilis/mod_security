<%= @disabled.include?("981198") ? "#" : "" %>SecRule &RESOURCE:OSVDB_CHECK "@eq 0" "chain,phase:5,id:'981198',t:none,nolog,pass"
<%= @disabled.include?("981198") ? "#" : "" %>        SecRule RESPONSE_STATUS "@streq 200" "exec:<%= @parameters['981198'] %>"
<%= @disabled.include?("981199") ? "#" : "" %>SecRule TX:OSVDB_MSG "!^$" "phase:5,id:'981199',t:none,log,pass,msg:'Passive Vulnerabilty Check with OSVDB - %{matched_var}'"
